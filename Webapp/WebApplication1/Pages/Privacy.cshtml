@page
@model PrivacyModel
@{
    ViewData["Title"] = "Informationen zu den Organisationsformen";
}
<!DOCTYPE html>
<html lang="de">
<head>
    <!-- Der Head-Bereich enthält üblicherweise Metadaten und externe Ressourcen -->
</head>
<body>
    <header>
        <h1>@ViewData["Title"]</h1>
        <!-- Der Titel des Dokuments wird dynamisch aus ViewData generiert -->
        <title>Image Overlay Example</title>
        <!-- Titel des Dokuments (statisch) -->
        <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
        <!-- Verweis auf jQuery UI-Stile -->
        <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
        <!-- Verweis auf jQuery-Bibliothek -->
        <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
        <!-- Verweis auf jQuery UI-Bibliothek -->
        <style>
            /* Hier beginnt die eingebettete CSS-Stildefinition */
            body {
                /* Stile für den gesamten Körper des Dokuments */
                display: flex;
                flex-direction: column;
                align-items: center;
            }

            nav {
                /* Stile für die Navigationsleiste hier */
            }

            /* Weitere CSS-Stile für verschiedene Elemente */
        </style>
    </header>

    <main>
        <div class="privacyText">
            <!-- Hier beginnt der Hauptinhalt der Seite -->
            <!-- Beispieltexte für verschiedene Abschnitte -->
            <h2>Schwarm</h2>
            <!-- Überschrift für den Abschnitt "Schwarm" -->
            <p>Mit dem Punkte Symbol kann die Organisationsform Schwarm dargestellt werden...</p>
            <!-- Text für den Abschnitt "Schwarm" -->
            <!-- ... Weitere Überschriften und Texte für andere Abschnitte ... -->
        </div>
    </main>

    <script>
        // Hier könnten JavaScript-Anweisungen stehen, falls benötigt
    </script>
</body>
</html>